@model Extra


<div class="card" style="width: 20rem;">
    <img src="https://thumbs.dreamstime.com/z/placeholder-rgb-color-icon-image-gallery-photo-thumbnail-available-album-digital-media-multimedia-file-visual-content-snapshot-187369540.jpg" class="card-img-top" alt="...">
    <div class="card-body">
        <h4 class="card-title">Name: @Model.ExtraName</h4>
        <h4>
            Price: @Model.ExtraPrice
        </h4>
        <div class="row">
            <a class="btn btn-primary"
               asp-controller="Extra"
               asp-action="List">Back</a>
        </div>
    </div>
</div>
